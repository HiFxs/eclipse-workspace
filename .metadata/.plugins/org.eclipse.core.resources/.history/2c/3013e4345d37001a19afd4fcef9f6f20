//输入
//[1,0,2,3,0,4,5,0]
//输出
//[1,0,0,3,3,3,3,3]
//预期结果
//[1,0,0,2,3,0,0,4]
class Solution {
//	0,0,1,2,3,4,4,5,5,6
    public int removeElement(int[] nums, int val) {
     Arrays.sort(nums);
     int k=0,length=nums.length;
     for(int i=0;i<length;i++) {
    	 if(nums[i]!=val) {
    		 nums[k]=nums[i];
    		 k++;
    	 }
    	 
    	 
     }


return k+1;

        
    }
}

